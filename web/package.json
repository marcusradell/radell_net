{
  "name": "web",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@storybook/addon-docs": "^5.3.0-beta.20",
    "@testing-library/jest-dom": "^4.2.4",
    "@testing-library/react": "^9.3.2",
    "@testing-library/user-event": "^7.1.2",
    "@types/jest": "^24.0.0",
    "@types/node": "^12.0.0",
    "@types/react": "^16.9.0",
    "@types/react-dom": "^16.9.0",
    "react": "^16.12.0",
    "react-dom": "^16.12.0",
    "react-scripts": "3.3.0",
    "tailwindcss": "^1.1.4",
    "typescript": "~3.7.2"
  },
  "scripts": {
    "dev": "npm run tailwind:css && react-scripts start",
    "tailwind:css": "tailwind build src/tailwind.css -c tailwind.config.js -o src/index.css",
    "build": "npm run tailwind:css && react-scripts build",
    "test": "react-scripts test",
    "deploy": "npm run build && firebase deploy",
    "storybook": "npm run tailwind:css && start-storybook -p 9009 -s public",
    "build-storybook": "npm run tailwind:css && build-storybook -s public"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@storybook/addon-actions": "^5.3.0-beta.20",
    "@storybook/addon-links": "^5.3.0-beta.20",
    "@storybook/addons": "^5.3.0-beta.20",
    "@storybook/preset-create-react-app": "^1.3.2",
    "@storybook/react": "^5.3.0-beta.20"
  }
}
